@model IEnumerable<FileStorage.BLL.DTO.UserDTO>
<tr>
    <th>
        @Html.DisplayNameFor(model => model.Email)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.UserName)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.CurrentSize)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.MaxSize)
    </th>
    <th width="50px"></th>
    @if (User.IsInRole("admin"))
    {
        <th width="50px"></th>
        <th width="50px"></th>
    }
</tr>
