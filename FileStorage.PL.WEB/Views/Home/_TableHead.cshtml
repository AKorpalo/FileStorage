@model IEnumerable<FileStorage.BLL.DTO.FileInfoDTO>
<tr>
    <th>
        @Html.DisplayNameFor(model => model.FileName)
    </th>
    <th width="150px">
        @Html.DisplayNameFor(model => model.Size)
    </th>
    <th width="200px">
        @Html.DisplayNameFor(model => model.DownloadDate)
    </th>
    <th width="200px">
        @Html.DisplayNameFor(model => model.UserId)
    </th>
    <th width="50px"></th>
    @if (User.IsInRole("admin") || User.IsInRole("moderator"))
    {
        <th width="50px"></th>
    }
</tr>
